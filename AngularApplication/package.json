  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:3000": "ng serve --port 3000 --proxy-config dev.proxy.conf.json",
    "start:4200": "ng serve --port 4200 --proxy-config dev.proxy.conf.json",
    "build:stage": "ng build --env=stage",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:nginx": "docker-compose up -d --build",
    "stop:nginx": "docker-compose down ",
    "start:client": "ng serve --port 80",
    "start:deploy": "concurrently \"npm run stop:nginx\"  \"npm run build\"  \"npm run start:nginx\""
  },
  "private": true,
  "dependencies": {
